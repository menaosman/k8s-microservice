        </mxCell>

        <!-- End User -->
        <mxCell id="user" value="End User" style="shape=actor;fillColor=#d5e8d4;strokeColor=#82b366;" vertex="1" parent="1">
          <mxGeometry x="1100" y="200" width="60" height="80" as="geometry"/>
