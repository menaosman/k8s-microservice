        <mxCell id="c5" edge="1" parent="1" source="terraform" target="aws">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="c6" edge="1" parent="1" source="docker" target="k8s">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell id="c7" edge="1" parent="1" source="aws" target="k8s">
